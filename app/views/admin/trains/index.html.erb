<h1>Trains</h1>

<table class="table">
  <thead>
    <tr>
      <th>Number</th>
      <th>Route</th>
      <th>Current Station</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @trains %>
  </tbody>
</table>


<p>
  <%= link_to 'New Train', new_admin_train_path, class: 'btn btn-primary' %> |
  <%= link_to 'Back', admin_path, class: 'btn btn-warning' %>
</p>
